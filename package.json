{
  "name": "nonebot-registry",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:preview": "vue-tsc && vite build --mode preview",
    "preview": "vite preview",
    "eslint": "eslint --cache --report-unused-disable-directives",
    "eslint:fix": "pnpm eslint --fix",
    "stylelint": "stylelint",
    "stylelint:fix": "pnpm stylelint --fix",
    "lint": "pnpm lint:js && pnpm lint:style",
    "lint:fix": "pnpm lint:js:fix && pnpm lint:style:fix",
    "lint:js": "pnpm eslint \"**/*.{js,jsx,ts,tsx,cjs,mjs,vue}\"",
    "lint:js:fix": "pnpm eslint:fix \"**/*.{js,jsx,ts,tsx,cjs,mjs,vue}\"",
    "lint:style": "pnpm stylelint \"**/*.{css,scss}\"",
    "lint:style:fix": "pnpm stylelint:fix \"**/*.{css,scss}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdi/js": "^7.3.67",
    "@sentry/vue": "^7.81.1",
    "@vueuse/core": "^10.6.1",
    "ansi_up": "^6.0.2",
    "gsap": "^3.12.2",
    "highlight.js": "^11.9.0",
    "naive-ui": "^2.35.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.10.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@unocss/transformer-directives": "^0.57.7",
    "@vitejs/plugin-vue": "^4.5.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.9.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-prettier": "^4.1.0",
    "typescript": "^5.3.2",
    "unocss": "^0.57.7",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.22"
  }
}
